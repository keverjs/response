{
  "name": "@kever/response",
  "version": "1.2.1",
  "description": "standard response output format.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "keywords": [
    "response"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/keverjs/response.git"
  },
  "bugs": {
    "url": "https://github.com/keverjs/response/issues"
  },
  "scripts": {
    "build": "rimraf dist && unbuild",
    "format": "eslint --fix . --ext .js,.ts,.cjs,.mjs",
    "ci": "bumpp package.json --commit --push --tag",
    "release": "run-s build format ci",
    "docs": "typedoc",
    "deploy": "esmo ./scripts/deploy.ts",
    "deploy:docs": "run-s docs deploy",
    "test:unit": "jest",
    "test:dts": "echo 'test:dts'",
    "test": "run-s test:unit test:dts"
  },
  "devDependencies": {
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "bumpp": "^7.1.1",
    "chalk": "^5.0.0",
    "eslint": "^8.10.0",
    "esmo": "^0.14.1",
    "jest": "^27.5.1",
    "jest-useragent-mock": "^0.1.1",
    "kever": "^0.26.0",
    "lint-staged": "^12.3.5",
    "npm-run-all": "^4.1.5",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "tsup": "^5.12.1",
    "typedoc": "^0.22.13",
    "typedoc-theme-category": "^1.0.7",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  },
  "peerDependencies": {
    "kever": ">=0.25.4"
  },
  "homepage": "https://github.com/keverjs/response#readme",
  "author": "hubvue",
  "license": "MIT"
}
